        <li class="order-summary-item border-0">
            <div class="order-summary-item-label">{{ 'cart.total-amount-paid'|trans }}</div>
            <div class="JS--summaryTotalPos order-summary-total" data-summary-total="{{ order.summary.totalAmountToPay }}">
                <div class="JS--summaryTotal text-nowrap">{{ order.summary.totalAmountToPay|money }}</div>
            </div>
