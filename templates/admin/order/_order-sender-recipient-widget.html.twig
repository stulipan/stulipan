    <div id="sender_recipient" class="">
        <div class="row">
            <div class="col-12 col-sm-6 h-100">
                <div class="d-flex justify-content-between align-items-center">
                    <div class="h5 mb-0">{{ 'order.sender'|trans }}</div>
                    {% if isEditEnabled is defined and isEditEnabled == true %}
                        <div class="edit-block d-print-none">
                            <div class="d-flex align-items-center">
                                <a class="btn-link-secondary" href="" data-toggle="modal" data-target="#modal--Sender" data-backdrop="static" data-url="{{ path('order-editBillingInfo', {id: order.id}) }}"><i class="far fa-edit mr-0"></i></a>
                            </div>
                        </div>
                    {% endif %}
                </div>
                <div class="mt-2">
                    <div class="h-100">
                        <div class="mb-1 font-weight-bold text-primary">
                            {% if order.billingFullname is defined and order.billingFullname is not null %}
                                {{ order.billingFullname }}
                                {% if order.billingCompany is defined and order.billingCompany %}
                                    <br>
                                    {{ order.billingCompany }}
                                {% endif %}
                            {% else %}
                                {% include 'admin/_missing-data-insert.html.twig' %}
                            {% endif %}
                        </div>
                        <div class="">
                            <span class="">
                                {% if order.billingAddress is defined and order.billingAddress %}
                                    {{ order.billingAddress.zip }} {{ order.billingAddress.city }},<br> {{ order.billingAddress.street }}
                                    <br>
                                    <span class="text-muted">{{ order.billingAddress.province }}, {{ order.billingAddress.country }}</span>
                                {% else %}
                                    {% include 'admin/_missing-data-insert.html.twig' %}
                                {% endif %}
                            </span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-12 col-sm-6 mt-4 mt-sm-0 h-100">
                <div class="d-flex justify-content-between align-items-center">
                    <div class="h5 mb-0">{{ 'order.recipient'|trans }}</div>
                    {% if isEditEnabled is defined and isEditEnabled == true %}
                        <div class="edit-block d-print-none">
                            <div class="d-flex">
                                <a class="btn-link-secondary"  href="" data-toggle="modal" data-target="#modal--Recipient" data-backdrop="static" data-url="{{ path('order-editShippingInfo', {id: order.id}) }}"><i class="far fa-edit mr-0"></i></a>
                            </div>
                        </div>
                    {% endif %}
                </div>
                <div class="recipient mt-2">
                    <div class="h-100">
                        <div class="mb-1 font-weight-bold text-success">
                            {% if order.shippingFullname is defined and order.shippingFullname is not null %}
                                {{ order.shippingFullname }}
                            {% else %}
                                {% include 'admin/_missing-data-insert.html.twig' %}
                            {% endif %}
                        </div>
                        <div class="">
                                <span class="">
                                    {{ 'order.phone-number'|trans }}
                                    {% if order.shippingPhone is defined and order.shippingPhone %}
                                        <em>{{ order.shippingPhone }}</em>
                                    {% else %}
                                        {% include 'admin/_missing-data-insert.html.twig' %}
                                    {% endif %}
                                </span>
                            <br>
                            <span class="">
                                    {% if order.shippingAddress is defined and order.shippingAddress %}
                                        {{ order.shippingAddress.zip }} {{ order.shippingAddress.city }},<br> {{ order.shippingAddress.street }}
                                        <br>
                                        <span class="text-muted">{{ order.shippingAddress.province }}, {{ order.shippingAddress.country }}</span>
                                    {% else %}
                                        {% include 'admin/_missing-data-insert.html.twig' %}
                                    {% endif %}
                                </span>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>