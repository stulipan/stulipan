            <div>{{  order.summary.totalBeforeDiscount|money }}</div>
        </li>
        <li class="order-summary-item">
            <div class="order-summary-item-label">{{ 'cart.shipping-fee'|trans }}</div>
            <div>
                                    <div class="JS--summaryDeliveryFee">{{ order.deliveryFee|money }}</div>
                                </div>
        </li>
